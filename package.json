{
  "private": true,
  "devDependencies": {
    "gulp": "^3.8.8",
    "hammerjs": "^2.0.6"
  },
  "dependencies": {
    "bootstrap-sass": "^3.0.0",
    "laravel-elixir": "^4.0.0",
    "materialize-css": "^0.97.5"
  },
  "browserify": {
    "transform": [
      "browserify-shim"
    ]
  },
  "browser": {
    "jquery": "./node_modules/jquery/dist/jquery.min.js",
    "materialize-js": "./node_modules/materialize-css/bin/materialize.js"
  },
  "browserify-shim": {
    "angular": "angular",
    "jquery": "$",
    "materialize-js": {
      "exports": "Materialize",
      "depends": [
        "jquery:jQuery"
      ]
    }
  }
}
